<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="RegisterMap">

<insert id="registerJoin" parameterType="member">
	insert into member(mem_num,mem_name,mem_pass,mem_tel,mem_email)
	values(#{mem_num},#{mem_name},#{mem_pass},#{mem_tel},#{mem_email})
</insert>

<select id="login" parameterType="member" resultType="member">
	select *
	from member
	where Mem_id = #{mem_num} and MEM_PASS= #{mem_pass}
</select>




</mapper>