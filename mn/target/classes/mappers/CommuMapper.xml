<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CommuMap">

<insert id="CommunityInsert" parameterType="board" >

insert into board(bo_num,mem_num,bo_title,bo_content,bo_date,bo_cate) values(board_seq.NEXTVAL,member_seq.NEXTVAL,#{bo_title},#{bo_content},sysdate,#{bo_cate})

</insert>

<select id="getBoardList" resultType="board">
select * from board
</select>

</mapper>
