<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CommuMap">

<insert id="CommunityInsert" parameterType="CommunityVO" >

insert into board(bo_num,mem_id,bo_title,bo_content,bo_date) values(BOARD_SEQ.nextval,#{mem_id},#{bo_title},#{bo_content},sysdate)

</insert>

<select id="getBoardList" resultType="CommunityVO">
select * from board
</select>

</mapper>
